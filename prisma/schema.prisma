
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id Int @default(autoincrement())
  email String @unique
  username String @unique
  password String @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}